{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "build": {
    "env": {
      "PIP_NO_CACHE_DIR": "off"
    },
    "commands": [
      "pip install -r requirements.txt"
    ]
  }
}
